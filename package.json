{
  "name": "audible-hero",
  "description": "Series",
  "version": "1.0.0",
  "author": "Eirik Brandtz√¶g <eirikb@eirikb.no>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --https",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "ext": "shx rm -rf dist audiblehero.zip && shx mkdir -p dist && shx cp -r ext/* dist/",
    "zip": "cd dist; zip ../audiblehero.zip *",
    "prod": "npm-run-all ext build zip",
    "start": "npm-run-all ext dev"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "glob": "7.1.2",
    "lodash": "4.17.4",
    "lz-string": "1.4.4",
    "material-components-web": "0.22.0",
    "npm-run-all": "4.1.1",
    "scss-loader": "0.0.1",
    "vue": "2.5.2",
    "vue-router": "3.0.1",
    "vuex": "3.0.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-latest": "6.24.1",
    "cross-env": "5.1.0",
    "css-loader": "0.28.7",
    "file-loader": "1.1.5",
    "node-sass": "4.5.3",
    "sass-loader": "6.0.6",
    "shx": "0.2.2",
    "vue-loader": "11.1.4",
    "vue-template-compiler": "2.5.2",
    "webpack": "3.7.1",
    "webpack-dev-server": "2.9.2"
  }
}
